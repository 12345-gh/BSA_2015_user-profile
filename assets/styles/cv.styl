.v-center
  display table;
  span
    display table-cell
    text-align right
    vertical-align middle
    a
      color #8D8D8D
      font-weight bold
      text-decoration none !important
      &:hover
        color #333

.btn-rmv-tech, .btn-rmv-project, .btn-edit-project
  background none !important
  border 0 !important
  cursor pointer
  outline none

.btn-rmv-project, .btn-edit-project
  margin-left 5 px

.btn-rmv-project
  &:hover
    color #E60E0E

.btn-edit-project
  &:hover
    color #337ab7;

.technology
  &:hover
    .btn-rmv-tech
      display inline-block !important

.project
  position relative

.project-header
  cursor pointer

#inputFile
  font-size 14px !important


